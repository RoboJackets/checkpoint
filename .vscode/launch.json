    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flask",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "checkpoint.py",
                "FLASK_DEBUG": "1",
                "FLASK_CACHE_TYPE": "SimpleCache"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true,
            "autoStartBrowser": false
        },
        {
            "name": "Migrate",
            "type": "debugpy",
            "request": "launch",
            "module": "migrate",
            "env": {},
            "envFile": "${workspaceFolder}/.env",
            "args": [],
            "jinja": true,
            "autoStartBrowser": false
        }
    ]
}